<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/navigation_main_fragment">

    <fragment
        android:id="@+id/navigation_main_fragment"
        android:name="com.promise09th.mvvmkotlin.presentation.main.MainFragment"
        android:label="MainFragment">
        <action
            android:id="@+id/action_mainFragment_to_detailFragment"
            app:destination="@id/navigation_detail_fragment" />
    </fragment>

    <fragment
        android:id="@+id/navigation_detail_fragment"
        android:name="com.promise09th.mvvmkotlin.presentation.main.DetailFragment"
        android:label="DetailFragment" />

</navigation>
